public class Matrix_add{     

	public static void main(String[] args) {
		int a[][]= { {1,3,4} , {2,4,3 } ,{ 3,4,5} };
		int b[][]= { {1,3,4} , {2,4,3 } ,{ 1,2,4} };
		int c[][] = new int[3][3];
		for(int i=0;i<3;i++)
		{
		for(int j=0;p<3;p++)
		{	c[i][j]=a[i][p] +b[j][p];
		System.out.print(c[i][j]+" ");
		}
		System.out.println();
		}
	}

}
