public static void main(String args[])  
	{    
	 int a1=0,a2=1,a3,count; 
	 Scanner sc = new Scanner (System.in);
		System.out.println("Enter the total number of fibonacci numbers wanted");
		 count=sc.nextInt();       
	 System.out.print(a1+" "+n2);    
	    
	 for(int i=2;i<count;++i)
	 {    
	  n3=n1+n2;    
	  System.out.print(" "+n3);    
	  n1=n2;    
	  n2=n3;    
	 }    
	  
	}
